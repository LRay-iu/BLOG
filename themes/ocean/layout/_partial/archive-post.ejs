<article class="archive-article archive-type-<%= post.layout %>">
  
  <% if(post.photos.length){ %>
    <div class="archive-article-gallery "  style="max-width: 150px;max-height: 150px;margin:5px 20px 0 10px;">
      <% post.photos.forEach(function(photo, i){ %>
        <a class="article-gallery-img" href="<%- url_for(post.path) %>" rel="gallery_<%= post._id %>">
          <img src="<%- url_for(photo) %>" itemprop="image" style="border-radius: 15px;">
        </a>
      <% }) %>
    </div>
  <% } %>
  
  <div class="archive-article-inner">
    <header class="archive-article-header ">
      <%- partial('post/date', {class_name: 'archive-article-date', date_format: 'MM/DD'}) %>
      <%- partial('post/title', {class_name: 'archive-article-title'}) %>
    </header>
      <!-- <%- post.excerpt %> -->
    <% if(page.category){ %>
      <%- partial('post/category') %>
    <% } %>
    <% if(page.tag){ %>
      <%- partial('post/tag') %>
    <% } %>
  </div>


</article>